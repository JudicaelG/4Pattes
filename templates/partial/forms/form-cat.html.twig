{{form_start(formCat)}}
        <div class="grid grid-cols-1 lg:grid-cols-2 gap-3.5">
        <div class="mx-2 my-2">                    
            <label class="block font-bold">{{field_label(formCat.name)|trans}}</label>                    
            <input name="{{field_name(formCat.name)}}" value="{{field_value(formCat.name)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
            focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none" required>
            <span class="error-form">{{ form_errors(formCat.name) }}</span>
        </div>
        <div class="mx-2 my-2">
            <label class="block font-bold">{{field_label(formCat.birthday)|trans}}</label>
            <input type="date" name="{{field_name(formCat.birthday)}}" value="{{field_value(formCat.birthday)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
            focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none" required>
            <span class="error-form">{{ form_errors(formCat.birthday) }}</span>
        </div>
    </div>
    <div class="grid grid-cols-1 lg:grid-cols-3">
        <div class="mx-2 my-2">
            <label class="block font-bold">{{field_label(formCat.weight)|trans}}</label>
            <input type="number" step="0.1" name="{{field_name(formCat.weight)}}" value="{{field_value(formCat.weight)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
                focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none" required>
            <span class="error-form">{{ form_errors(formCat.weight) }}</span>
        </div>
        <div class="mx-2 my-2">
            <label class="block font-bold">{{field_label(formCat.sexe)|trans}}</label>
            <select name="{{field_name(formCat.sexe)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
                focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none">
                {% for label, value in field_choices(formCat.sexe) %}
                    <option value="{{ value }}">{{ label }}</option>
                {% endfor %}
            </select>            
            <span class="error-form">{{ form_errors(formCat.sexe) }}</span>
        </div>
        <div class="mx-2 my-2">
            <label class="block font-bold">{{field_label(formCat.sterilized)|trans}}</label>
            <select name="{{field_name(formCat.sterilized)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
                focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none">
                {% for label, value in field_choices(formCat.sterilized) %}
                    <option value="{{ value }}">{{ label }}</option>
                {% endfor %}
            </select>     
            <span class="error-form">{{ form_errors(formCat.sterilized) }}</span>
        </div>
    </div>
    <div class="grid grid-cols-1 lg:grid-cols-2">
        <div class="mx-2 my-2">
            <label class="block font-bold">{{field_label(formCat.breed_id)|trans}}</label>
            <select name="{{field_name(formCat.breed_id)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
                focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none">
                {% for label, value in field_choices(formCat.breed_id) %}
                    <option value="{{ value }}">{{ label }}</option>
                {% endfor %}
            </select>   
            <span class="error-form">{{ form_errors(formCat.breed_id) }}</span>
        </div>
        <div class="mx-2 my-2">
            <label class="block font-bold">{{field_label(formCat.profilePhoto)|trans}}</label>
            <input type="file" name="{{field_name(formCat.profilePhoto)}}" value="{{field_value(formCat.profilePhoto)}}" class="block w-full px-2 py-2">
            <span class="error-form">{{ form_errors(formCat.profilePhoto) }}</span>
        </div>    
    </div>
    <div class="mx-2 my-2 ">
        <h3 class="block mt-6 mb-3 font-bold">{% trans %}Vaccins{% endtrans %}</h3>
        <div class="grid grid-cols-4 gap-2.5">
            {% for vaccinated in formCat.vaccinateds %}
                <div class="">
                    {% for vaccine in vaccinated.vars.value.getVaccineId() %}
                        {{ form_label(formCat.name, vaccine.name, {
                            'label_attr': {'class': 'block font-bold'}
                        }) }}
                    {% endfor %}
                    {{ form_widget(vaccinated) }}
                    <span class="error-form">{{ form_errors(vaccinated) }}</span>
                </div>
                {% endfor %}
        </div>
    </div>
    <div class="mx-2 my-2">                 
        {% if formCat.add_veterinary is defined %}
            <h3 class="block mt-6 mb-3 font-bold">{% trans %}Ajouter un vétérinaire{% endtrans %}</h3>             
            <div class="grid grid-cols-2 gap-2.5">
                <div class="col-span-2">
                    <label class="block font-bold">{{field_label(formCat.add_veterinary.name)|trans}}</label>
                    <input type="text" name="{{field_name(formCat.add_veterinary.name)}}" value="{{field_value(formCat.add_veterinary.name)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
                        focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none">
                    <span class="error-form">{{ form_errors(formCat.add_veterinary.name) }}</span>
                </div>
                <div class="col-span-2">
                    <label class="block font-bold">{{field_label(formCat.add_veterinary.adress)|trans}}</label>
                    <input type="text" name="{{field_name(formCat.add_veterinary.adress)}}" value="{{field_value(formCat.add_veterinary.adress)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
                        focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none">
                    <span class="error-form">{{ form_errors(formCat.add_veterinary.adress) }}</span>
                </div>
                <div class="">
                    <label class="block font-bold">{{field_label(formCat.add_veterinary.postalCode)|trans}}</label>
                    <input type="number" name="{{field_name(formCat.add_veterinary.postalCode)}}" value="{{field_value(formCat.add_veterinary.postalCode)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
                        focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none">
                    <span class="error-form">{{ form_errors(formCat.add_veterinary.postalCode) }}</span>
                </div>
                <div class="">
                    <label class="block font-bold">{{field_label(formCat.add_veterinary.city)|trans}}</label>
                    <input type="text" name="{{field_name(formCat.add_veterinary.city)}}" value="{{field_value(formCat.add_veterinary.city)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
                        focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none">
                    <span class="error-form">{{ form_errors(formCat.add_veterinary.city) }}</span>
                </div>
                <div class="">
                    <label class="block font-bold">{{field_label(formCat.add_veterinary.phone)|trans}}</label>
                    <input type="tel" name="{{field_name(formCat.add_veterinary.phone)}}" value="{{field_value(formCat.add_veterinary.phone)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
                        focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none">
                    <span class="error-form">{{ form_errors(formCat.add_veterinary.phone) }}</span>
                </div>
            </div>                
        {% else %}
            <div>
                <label class="block font-bold">{{field_label(formCat.veterinary)|trans}}</label>
                <select name="{{field_name(formCat.veterinary)}}" class="block w-full px-2 py-2 rounded-lg bg-white shadow-lg text-gray-700 
                focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none">
                {% for label, value in field_choices(formCat.veterinary) %}
                    <option value="{{ value }}">{{ label }}</option>
                {% endfor %}
            </select> 
                <span class="error-form">{{ form_errors(formCat.veterinary) }}</span>
            </div>
        {% endif %}
    </div>
    <div class="my-2 mx-2 float-right">
        <button type="submit" id="{{field_label(formCat.save)}}" name="{{field_name(formCat.save)}}" class="bg-primary px-4 py-3 rounded-full focus:border-primary focus:ring-1 focus:ring-primary focus:outline-none text-white min-w-11">
            {% trans %}Ajouter{% endtrans %}
        </button>
    </div>
        {{form_rest(formCat)}}
{{form_end(formCat)}}